source 'https://rubygems.org'
ruby '2.4.1'

# Bundle edge Rails instead: gem 'rails', github: 'rails/rails'
gem 'rails', '4.2.8'

# Database gems 
gem 'sqlite3', group: :development
gem 'mysql2', '~> 0.4.8', group: :production
